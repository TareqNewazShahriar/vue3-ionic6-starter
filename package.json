    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.43.0",
    "eslint-plugin-vue": "^9.15.1",
    "typescript": "^5.1.5",
    "vite": "^4.3.9",
    "vue-tsc": "^1.8.3"
